<templateSet group="PHP">
  <template name="dd" value="dd($END$);" description="debug_laravel" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="fori" value="foreach ($ITERABLE$ as $VAR_VALUE$) {&#10;    $END$&#10;}" shortcut="TAB" description="foreach(iterable_expr as $value) {...}" toReformat="false" toShortenFQNames="true">
    <variable name="ITERABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_VALUE" expression="phpSuggestVariableName()" defaultValue="&quot;$item&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="de" value="utilDebug::preprint($END$);exit();" description="debug" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>