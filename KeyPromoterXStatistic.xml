<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close All">
        <Item>
          <IdeaActionID>CloseAllEditors</IdeaActionID>
          <ShortCut>'Ctrl+Shift+W' or 'Ctrl+Shift+F4'</ShortCut>
          <Description>Close All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'Shift+Enter'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>17</Count>
        </Item>
      </Statistic>
      <Statistic Action="Edit">
        <Item>
          <IdeaActionID>Console.TableResult.EditValue</IdeaActionID>
          <ShortCut>'F2' or 'Enter' or 'Alt+Enter'</ShortCut>
          <Description>Edit</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Execute">
        <Item>
          <IdeaActionID>Console.Jdbc.Execute</IdeaActionID>
          <ShortCut>'Ctrl+Enter'</ShortCut>
          <Description>Execute</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+1' or 'Ctrl+2' or 'Ctrl+3' or 'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Remove">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Excluir'</ShortCut>
          <Description>Remove</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Set NULL">
        <Item>
          <IdeaActionID>Console.TableResult.SetNull</IdeaActionID>
          <ShortCut>'Ctrl+Alt+N'</ShortCut>
          <Description>Set NULL</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+7' or 'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>29</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>